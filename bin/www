#!/usr/bin/env node
//var config = require('./config');
var debug = require('debug')('geocoding-service');
var app = require('../app/app');

app.set('port', process.env.PORT || 9600); //config.server['port']);
console.log('---');
debug('app init');


var server = app.listen(app.get('port'), function() {
  debug('listening port ' + server.address().port);
});
